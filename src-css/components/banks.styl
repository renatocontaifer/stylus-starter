/* Banks */

.banks
  padding-top 0

.section-tit
  background $roxo
  color #fff
  padding 10px 20px
  font-size 20px
  text-align center
  +above(540px)
    text-align center
  +above(768px)
    padding 20px

.banks-list
  +above(768px)
    max-width 700px
    margin 0 auto

.banks-item
  margin-bottom 20px
  text-align center
  +above(600px)
    column(1/3)
    margin-top 20px
  img
    max-width 190px
    height auto
